        <Param val="" valType="extendedCode" updates="constant" name="Before Experiment"/>
        <Param val="" valType="extendedCode" updates="constant" name="Before JS Experiment"/>
        <Param val="# Import necessary module&amp;#10;from psychopy import core&amp;#10;&amp;#10;# Initialize parameters for the cue&amp;#10;initial_radius = 0.7  # Starting size of the cue&amp;#10;final_radius = 0.2    # Final size of the cue&amp;#10;duration = 2.0        # Time in seconds to shrink&amp;#10;hold_time = 1.5       # Time in seconds to hold after reaching final size&amp;#10;&amp;#10;cue_started = False    # To track when the cue starts shrinking&amp;#10;cue_start_time = None  # To store when the cue started shrinking&amp;#10;" valType="extendedCode" updates="constant" name="Begin Experiment"/>
        <Param val="import {core} from 'psychopy';&amp;#10;initial_radius = 0.7;&amp;#10;final_radius = 0.2;&amp;#10;duration = 2.0;&amp;#10;hold_time = 1.5;&amp;#10;cue_started = false;&amp;#10;cue_start_time = null;&amp;#10;" valType="extendedCode" updates="constant" name="Begin JS Experiment"/>
        <Param val="cue_started = false;&amp;#10;cue_start_time = null;&amp;#10;" valType="extendedCode" updates="constant" name="Begin JS Routine"/>
        <Param val="cue_started = False&amp;#10;cue_start_time = None" valType="extendedCode" updates="constant" name="Begin Routine"/>
        <Param val="Auto-&gt;JS" valType="str" updates="None" name="Code Type"/>
        <Param val="# Get the elapsed time since the routine started&amp;#10;elapsed_time = t  # 't' is automatically provided by PsychoPy as the time since the start of the routine&amp;#10;&amp;#10;# Hide the cue before 1 second&amp;#10;if elapsed_time &lt; 1.0:&amp;#10;    cue.fillColor = [0, 0, 0]  # Ensure it stays black&amp;#10;    cue.size = (0.7, 0.7)  # Reset size to initial, in case of flickering&amp;#10;else:&amp;#10;    # Start drawing the cue after 1 second&amp;#10;    if not cue_started:&amp;#10;        cue_start_time = elapsed_time  # Record the time when the cue starts shrinking&amp;#10;        cue_started = True&amp;#10;&amp;#10;    # Calculate how much time has passed since the cue started shrinking&amp;#10;    time_passed = elapsed_time - cue_start_time  # Time since cue started shrinking&amp;#10;&amp;#10;    # Calculate the new size based on the elapsed time&amp;#10;    new_radius = initial_radius - (time_passed / duration) * (initial_radius - final_radius)&amp;#10;&amp;#10;    # Ensure the radius does not go below the final size&amp;#10;    new_radius = max(new_radius, final_radius)&amp;#10;&amp;#10;    # Update the cue's size&amp;#10;    cue.size = (new_radius, new_radius)&amp;#10;&amp;#10;    # Change color when the cue reaches the target size&amp;#10;    if new_radius &lt;= final_radius:  # If cue size matches target size&amp;#10;        cue.fillColor = [0, 1, 0]  # Change color to green (using RGB values)&amp;#10;&amp;#10;    # Draw the cue only after 1 second and during the hold time&amp;#10;    cue.draw()  # Draw the cue&amp;#10;&amp;#10;    # End the routine after holding time&amp;#10;    if (cue.fillColor == [0, 1, 0]).all() and (time_passed &gt;= duration + hold_time):&amp;#10;        continueRoutine = False  # Ends the routine after holding&amp;#10;&amp;#10;# Draw the cue only after 1 second&amp;#10;if elapsed_time &gt;= 1.0:&amp;#10;    cue.draw()  # Draw the cue if the elapsed time is greater than or equal to 1 second&amp;#10;&amp;#10;# Flip the window to update the frame&amp;#10;win.flip()&amp;#10;" valType="extendedCode" updates="constant" name="Each Frame"/>
        <Param val="elapsed_time = t;&amp;#10;if ((elapsed_time &lt; 1.0)) {&amp;#10;    cue.fillColor = [0, 0, 0];&amp;#10;    cue.size = [0.7, 0.7];&amp;#10;} else {&amp;#10;    if ((! cue_started)) {&amp;#10;        cue_start_time = elapsed_time;&amp;#10;        cue_started = true;&amp;#10;    }&amp;#10;    time_passed = (elapsed_time - cue_start_time);&amp;#10;    new_radius = (initial_radius - ((time_passed / duration) * (initial_radius - final_radius)));&amp;#10;    new_radius = Math.max(new_radius, final_radius);&amp;#10;    cue.size = [new_radius, new_radius];&amp;#10;    if ((new_radius &lt;= final_radius)) {&amp;#10;        cue.fillColor = [0, 1, 0];&amp;#10;    }&amp;#10;    cue.draw();&amp;#10;    if (((cue.fillColor === [0, 1, 0]).all() &amp;&amp; (time_passed &gt;= (duration + hold_time)))) {&amp;#10;        continueRoutine = false;&amp;#10;    }&amp;#10;}&amp;#10;if ((elapsed_time &gt;= 1.0)) {&amp;#10;    cue.draw();&amp;#10;}&amp;#10;psychoJS.window.flip();&amp;#10;" valType="extendedCode" updates="constant" name="Each JS Frame"/>
